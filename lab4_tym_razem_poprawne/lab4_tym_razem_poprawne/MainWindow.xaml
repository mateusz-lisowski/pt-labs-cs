<Window x:Class="lab4_tym_razem_poprawne.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab4_tym_razem_poprawne"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <ToolBar DockPanel.Dock="Top">
                <ComboBox x:Name="propertyComboBox" Width="100" Margin="5"/>
                <TextBox x:Name="valueTextBox" Width="100" Margin="5"/>
                <Button Content="Search" Click="SearchButton_Click" Margin="5"/>
            </ToolBar>
            <DataGrid x:Name="dataGridView1" AutoGenerateColumns="True" CanUserAddRows="True" CanUserDeleteRows="True"/>
        </DockPanel>
    </Grid>
</Window>
